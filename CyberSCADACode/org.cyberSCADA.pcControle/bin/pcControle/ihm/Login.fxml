<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pcControle.controller.LoginController">
   <children>
      <Button fx:id="effacer" layoutX="150.0" layoutY="290.0" mnemonicParsing="false" onAction="#clearEvent" prefHeight="30.0" prefWidth="120.0" text="Effacer">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="connexion" layoutX="297.0" layoutY="290.0" mnemonicParsing="false" onAction="#loginEvent" prefHeight="30.0" prefWidth="175.0" text="Connexion">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <PasswordField fx:id="mdp" layoutX="300.0" layoutY="229.0">
         <font>
            <Font size="14.0" />
         </font>
      </PasswordField>
      <Label fx:id="titre" alignment="CENTER" layoutX="167.0" layoutY="62.0" prefHeight="53.0" prefWidth="266.0" text="Bienvenue">
         <font>
            <Font name="Serif Bold" size="30.0" />
         </font>
      </Label>
      <Label fx:id="idL" layoutX="150.0" layoutY="156.0" prefHeight="29.0" prefWidth="120.0" text="Identifiant">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="mdpL" layoutX="150.0" layoutY="229.0" prefHeight="29.0" prefWidth="136.0" text="Mot de Passe">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="id" layoutX="300.0" layoutY="156.0">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <Button fx:id="en" layoutX="513.0" layoutY="14.0" mnemonicParsing="false" onAction="#langEn" prefHeight="25.0" prefWidth="73.0" text="En" />
      <Button fx:id="fr" layoutX="426.0" layoutY="14.0" mnemonicParsing="false" onAction="#langFr" prefHeight="25.0" prefWidth="73.0" text="Fr" />
   </children>
</AnchorPane>
